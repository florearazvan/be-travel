<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Hyperlink?>
<VBox alignment="CENTER" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.itec.holzfaller.controllers.ReportController">
    <HBox alignment="CENTER">
        <Label text="For user: " />
        <ComboBox fx:id="userList" />
    </HBox>
    <Region prefHeight="10" prefWidth="Infinity" />
    <Button fx:id="createReport" text="Create Report" />
    <Region prefHeight="10" prefWidth="Infinity" />
    <Label fx:id="reportLocation" text="" />
    <Region prefHeight="10" prefWidth="Infinity" />
    <Label fx:id="reportForUser" text="" />
    <Region prefHeight="10" prefWidth="Infinity" />
    <Hyperlink fx:id="sendEmail" text="" disable="true" />
</VBox>
